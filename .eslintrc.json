{
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended"
  ],
  "env": {
    "node": true,
    "jquery": true
  },
  "rules": {
    "semi": "error",
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "vue/max-attributes-per-line": ["error", {
      "singleline": 3
    }]
  }
}
